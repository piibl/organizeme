CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE SEQUENCE END_USER_SEQ AS INTEGER START WITH 12 
CREATE SEQUENCE TASK_SEQ AS INTEGER START WITH 1 
CREATE SEQUENCE NOTE_SEQ AS INTEGER START WITH 1 
CREATE SEQUENCE SUB_TASK_SEQ AS INTEGER START WITH 1 
CREATE SEQUENCE NOTE_TAG_SEQ AS INTEGER START WITH 1 
CREATE SEQUENCE TAGSEQ AS INTEGER START WITH 50 INCREMENT BY 50 
CREATE MEMORY TABLE NOTE(ID BIGINT NOT NULL PRIMARY KEY,ARCHIVED BOOLEAN,CREATION_DATE TIMESTAMP,TITLE VARCHAR(255),TASK_ID BIGINT,END_USER_ID BIGINT)
CREATE MEMORY TABLE TAG(ID BIGINT NOT NULL PRIMARY KEY,COLOR VARCHAR(255),CREATION_DATE TIMESTAMP,LABEL VARCHAR(255))
CREATE MEMORY TABLE SUB_TASK(ID BIGINT NOT NULL PRIMARY KEY,ARCHIVED BOOLEAN,CREATION_DATE TIMESTAMP,GOAL VARCHAR(255),LOCALIZATION VARCHAR(255),PRIORITY VARCHAR(255),TASK_ID BIGINT)
CREATE MEMORY TABLE NOTE_TAG(ID BIGINT NOT NULL PRIMARY KEY,CREATION_DATE TIMESTAMP,NOTE_ID BIGINT,TAG_ID BIGINT,CONSTRAINT FK_NOTE_TAG_TAG_ID FOREIGN KEY(TAG_ID) REFERENCES TAG(ID),CONSTRAINT FK_NOTE_TAG_NOTE_ID FOREIGN KEY(NOTE_ID) REFERENCES NOTE(ID))
CREATE MEMORY TABLE END_USER(ID BIGINT NOT NULL PRIMARY KEY,CREATION_DATE TIMESTAMP,MAIL VARCHAR(255),FIRST_NAME VARCHAR(255),LAST_NAME VARCHAR(255),LOGIN VARCHAR(255),PWD VARCHAR(255))
CREATE MEMORY TABLE TASK(ID BIGINT NOT NULL PRIMARY KEY,ARCHIVED BOOLEAN,CREATION_DATE TIMESTAMP,GOAL VARCHAR(255),LOCALIZATION VARCHAR(255),PRIORITY VARCHAR(255),END_USER_ID BIGINT,CONSTRAINT FK_TASK_END_USER_ID FOREIGN KEY(END_USER_ID) REFERENCES END_USER(ID))
ALTER TABLE NOTE ADD CONSTRAINT FK_NOTE_TASK_ID FOREIGN KEY(TASK_ID) REFERENCES TASK(ID)
ALTER TABLE NOTE ADD CONSTRAINT FK_NOTE_END_USER_ID FOREIGN KEY(END_USER_ID) REFERENCES END_USER(ID)
ALTER TABLE SUB_TASK ADD CONSTRAINT FK_SUB_TASK_TASK_ID FOREIGN KEY(TASK_ID) REFERENCES TASK(ID)
